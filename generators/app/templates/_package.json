{
  "name": "generator-jhipster-<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "keywords": [
    "yeoman-generator",
    "jhipster-module"
  ],
  "homepage": "https://github.com/<%= githubName %>/generator-jhipster-<%= moduleName %>",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubName %>/generator-jhipster-<%= moduleName %>.git"
  },
  "dependencies": {
    "chalk": "2.1.0",
    "generator-jhipster": ">=4.6.0",
    "mkdirp": "0.5.1",
    "semver": "5.3.0",
    "shelljs": "0.5.3",
    "yeoman-generator": "2.0.0"
  },
  "devDependencies": {
    "eslint": "4.7.2",
    "eslint-config-airbnb-base": "12.0.0",
    "eslint-plugin-import": "2.7.0",
    "fs-extra": "4.0.2",
    "gulp": "3.9.1",
    "gulp-bump": "2.7.0",
    "gulp-eslint": "3.0.1",
    "gulp-exclude-gitignore": "1.1.1",
    "gulp-git": "2.1.0",
    "gulp-istanbul": "1.1.1",
    "gulp-mocha": "3.0.1",
    "gulp-nsp": "2.4.2",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sequence": "0.4.6",
    "gulp-shell": "0.6.3",
    "mocha": "3.5.3",
    "yeoman-assert": "3.1.0",
    "yeoman-test": "1.7.0"
  },
  "scripts": {
    "pretest": "eslint .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "mocha test/*"
  },
  <%_ if (license === 'apache') { _%>
  "license": "Apache-2.0",
  <%_ } else if (license === 'gpl') { _%>
  "license": "GPL-3.0",
  <%_ } else if (license === 'mit') { _%>
  "license": "MIT",
  <%_ } _%>
  "bugs": {
    "url": "https://github.com/<%= githubName %>/generator-jhipster-<%= moduleName %>/issues"
  }
}
