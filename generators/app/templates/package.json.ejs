{
  "name": "generator-jhipster-<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "keywords": [
    "yeoman-generator",
    "jhipster-module",
    "jhipster-7"
  ],
  "homepage": "https://github.com/<%= githubName %>/generator-jhipster-<%= moduleName %>",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubName %>/generator-jhipster-<%= moduleName %>.git"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "generator-jhipster": ">=7.0.0-beta.1",
    "mkdirp": "0.5.1",
    "semver": "7.1.1",
    "shelljs": "0.8.3",
    "yeoman-generator": "4.13.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "fs-extra": "9.1.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "mocha": "8.2.1",
    "prettier": "2.1.2",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "3.0.0"
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,**/}*.{js,json,md,yml}\"",
    "prettier:check": "prettier --check \"{,**/}*.{js,json,md,yml}\"",
    "pretest": "eslint .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release",
    "test": "mocha test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  <%_ if (license === 'apache') { _%>
  "license": "Apache-2.0",
  <%_ } else if (license === 'gpl') { _%>
  "license": "GPL-3.0",
  <%_ } else if (license === 'mit') { _%>
  "license": "MIT",
  <%_ } _%>
  "bugs": {
    "url": "https://github.com/<%= githubName %>/generator-jhipster-<%= moduleName %>/issues"
  }
}
